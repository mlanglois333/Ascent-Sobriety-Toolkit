<div class="start-content">
    <div class="signupJumbo box animate__animated animate__fadeIn animate__delay-0.5s">
        <button type="button" class="back fas fa-arrow-circle-left" value="Go back!" onclick="history.back()">
        </button>
        <div class="container register-container">
            <img SRC="img/logos/logo.png" alt="mountain logo"
                class="signin-logo animate__animated animate__fadeIn animate__delay-0.5s">
            <h2 class="signupTitle animate__animated animate__fadeIn animate__delay-0.5s">ASCENT</h2>
            <form class="animate__animated animate__fadeIn animate__delay-0.75s">
                <label class="inputLabel"> first name
                    <input type="text" placeholder="first name" id="name"> </label>
                <label class="inputLabel">email
                    <input type="text" placeholder="email" id="email"></label>
                <label class="inputLabel">password
                    <input type="password" placeholder="password" id="password"></label>
                <label class="inputLabel">sobriety date
                    <input type="date" name="html5date" id="soberDate"></label>
                <button class="btn btn-default login-btn" id="signUpBtn" type="button">Sign Up</button>
            </form>
        </div>
    </div>
</div>
{{!-- jQuery --}}
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

{{!-- momentjs --}}
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.25.1/moment.min.js"></script>

{{!-- Apex charts (can also do npm install) --}}
<script src="https://cdn.jsdelivr.net/npm/apexcharts"></script>

{{!-- Local Script --}}
<script src="js/signup.js"></script>